#!/bin/bash

name=superdesk-core
repo_remote=https://github.com/superdesk/superdesk-core.git

root=$(dirname $(dirname $(realpath -s $0)))
action=true . $root/superdesk-dev/master

do_backend() {
    _venv $env
    cd $repo
    pip install -U -r requirements.txt
}

do_checks() {
    _backend_checks
}

do_client_tests() { :; }
do_frontend() { :; }
do_finish() { :; }
do_prepopulate() { :; }

eval $action
